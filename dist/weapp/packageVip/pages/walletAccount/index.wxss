}

.wallet-account .cash .cash-btn {
  padding: 6rpx 36rpx;
  font-size: 24rpx;
  border-radius: 28rpx;
  border: 2rpx solid #ecad65;
  color: #ecad65;
}

.wallet-account .commission-modal {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  background: rgba(0, 0, 0, 0.7);
  z-index: 99999;
}

.wallet-account .commission-modal .commission-ct {
  width: 600rpx;
}

.wallet-account .commission-modal .commission-ct image {
  width: 600rpx;
}

.wallet-account .share-btn {
  background-color: transparent;
  text-align: left;
  line-height: normal;
  padding: 0;
}
