page {
  background: #f7f7f7;
}

.msg-list {
  color: #333333;
}

.msg-list .msg-box {
  background: #ffffff;
  margin: 16rpx;
  padding: 30rpx 30rpx 0 30rpx;
  border-radius: 4rpx;
}

.msg-list .msg-title {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  font-size: 30rpx;
  margin-bottom: 16rpx;
}

.msg-list .msg-content {
  font-size: 26rpx;
  color: #999999;
  margin-bottom: 16rpx;
}

.msg-list .msg-bt {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  font-size: 26rpx;
  border-top: 1rpx solid #f5f5f5;
  line-height: 60rpx;
  color: #999999;
}

.msg-list .success {
  color: #6493d7;
}

.msg-list .fail {
  color: #f75b5b;
}
