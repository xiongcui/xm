page {
  color: #333;
}

.editinfor,
page {
  background-color: #f7f8fa;
  height: 100%;
}

.editinfor {
  margin-top: 0;
}

.bottom_gap {
  height: 130rpx;
}

.avatar {
  text-align: right;
}

.item {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
}

.item_mid {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  width: 680rpx;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
}

.avatar image {
  background-color: #f0f0f0;
  border-radius: 50%;
  height: 60rpx;
  margin: 14rpx 0 0;
  width: 60rpx;
}

.personimg_label {
  color: #333;
  font-size: 30rpx;
  height: 160rpx !important;
  line-height: 160rpx !important;
}

.personimg {
  background-color: #f7f7f7;
  border-radius: 4rpx;
  display: inline-block;
  height: 120rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  position: relative;
  width: 84rpx;
}

.personimg_last {
  margin-right: 0;
}

.homeimg_label {
  color: #333;
  font-size: 30rpx;
}

.homeimg,
.homeimg_label {
  height: 140rpx !important;
  line-height: 140rpx !important;
}

.homeimg image {
  background-color: #f0f0f0;
  border-radius: 4rpx;
  height: 100rpx;
  margin: 20rpx 0 0;
  width: 125rpx;
}

.gap_mid,
.gap_top {
  height: 20rpx;
}

.infor {
  background-color: #fff;
  padding-left: 30rpx;
}

.item-b {
  border-bottom: 1rpx solid #eee;
}

.item-t {
  border-top: 1rpx solid #eee;
}

.item_icon {
  width: 60rpx;
}

.item_icon image {
  height: 40rpx;
  margin-top: 24rpx;
  width: 40rpx;
}

.item_input {
  color: #333;
  width: 60%;
}

.item_input,
.item_input_switch {
  font-size: 30rpx !important;
  height: 90rpx !important;
  line-height: 90rpx !important;
  padding-right: 20rpx;
  text-align: right;
}

.item_input_switch {
  color: #d5d5d5;
}

.item_input input {
  font-size: 30rpx !important;
  height: 88rpx !important;
  line-height: 88rpx !important;
  text-align: right;
}

.item_input button {
  background: #fff;
  border: none;
  color: #9b9b9b;
  font-size: 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  padding: 0;
  text-align: right;
}

.item_label,
.item_label_switch {
  color: #333;
  font-size: 30rpx;
  width: 5.5em;
}

.item_label,
.item_label_switch,
input {
  height: 90rpx !important;
  line-height: 90rpx !important;
}

input {
  border: none;
  font-size: 30rpx !important;
  width: 100%;
}

input:focus {
  outline: none;
}

.input-placeholder {
  color: #b2b2b2;
  font-size: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
}

input::-webkit-input-placeholder {
  color: #b1b1b1;
}

.item_right {
  text-align: left;
  width: 42rpx;
}

.item_right image {
  height: 22rpx;
  margin-top: 35rpx;
  width: 12rpx;
}

.item_right_homeimg image {
  margin-top: 60rpx;
}

.item_right_personimg image {
  margin-top: 67rpx;
}

.type_select {
  border: 1rpx solid #999;
  border-radius: 46rpx;
  color: #999;
  display: inline-block;
  font-size: 24rpx;
  height: 46rpx;
  line-height: 46rpx;
  margin-right: 40rpx;
  text-align: center;
  width: 108rpx;
}

.type_selected {
  background-color: #ff7487;
  border: 1rpx solid #ff7487;
  color: #fff;
}

button:focus {
  outline: none;
}

.pickers {
  text-align: right;
}

.pickers-right,
.pickers-right image {
  height: 40rpx;
  width: 40rpx;
}

.picker-none {
  color: #808080;
}

.pick-city {
  height: 100%;
  width: 100%;
}

.intro {
  background-color: #fff;
  height: 142rpx;
  margin-top: 20rpx;
  padding: 25rpx 30rpx;
  z-index: 1;
}

button::after {
  border: none;
}

.item_title {
  padding-left: 30rpx;
}

.ishow_title {
  color: #3196fa;
  font-size: 28rpx;
  height: 70rpx;
  line-height: 70rpx;
  width: 100%;
}

.item_input switch {
  height: 30rpx;
  margin-top: -10rpx;
}

.mid_gap,
.top_gap {
  height: 20rpx;
}

.title {
  color: #333;
  font-size: 26rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 31rpx;
  padding-top: 7rpx;
}

.title_edit {
  color: #999;
  font-size: 24rpx;
  padding-right: 20rpx;
  text-align: right;
  width: 110rpx;
}

.title_edit image {
  height: 19rpx;
  margin-right: 10rpx;
  width: 19rpx;
}

.infor_none {
  background: #fff;
  padding: 32rpx 0;
}

.infor_addbtn {
  border: 1rpx dotted #108ee9;
  border-radius: 10rpx;
  color: #108ee9;
  font-size: 26rpx;
  height: 66rpx;
  line-height: 66rpx;
  margin: 0 auto;
  text-align: center;
  width: 205rpx;
}

.work_exp {
  word-wrap: break-word;
  color: #9b9b9b;
  font-size: 30rpx;
  padding: 0 0 48rpx 31rpx;
  word-break: normal;
}

.zytags {
  background: #fff;
  padding: 30rpx 0 10rpx 31rpx;
}

.tag_itemed {
  background: #fff;
  border: 1rpx solid #999;
  border-radius: 4rpx;
  color: #333;
  display: inline-block;
  font-size: 24rpx;
  font-weight: 400;
  height: 57rpx;
  line-height: 56rpx;
  margin: 0 25rpx 20rpx 0;
  padding: 0 29rpx;
}

.item_content_fans {
  background: #fff;
  padding: 0 0 30rpx 30rpx;
}

.fans_item {
  float: left;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 30rpx;
  width: 50%;
}

.fans_item_icon image {
  height: 40rpx;
  width: 40rpx;
}

.fans_item_name {
  color: #333;
  padding-left: 20rpx;
}

.fans_item_status {
  color: #999;
  padding-left: 20rpx;
}

.intro_info {
  padding: 20rpx 0 20rpx 30rpx !important;
}

.intro_content {
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  font-size: 30rpx !important;
  min-height: 160rpx;
  overflow: hidden;
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
  width: 100%;
}

.intro_content_none {
  color: #959595;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
  min-height: 200rpx;
  padding: 20rpx 5rpx;
}

.intro_content text {
  width: 100%;
  word-break: break-all;
}

.bottom_gap {
  height: 180rpx;
}
