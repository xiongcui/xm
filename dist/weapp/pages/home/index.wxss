  background-color: #fff;
  height: 88rpx;
  white-space: nowrap;
  width: 100%;
  z-index: 99;
}

.swiper_tab ::-webkit-scrollbar {
  color: transparent;
  display: none;
  height: 0;
  width: 0;
}

.nav_list_ct {
  display: -webkit-flex;
