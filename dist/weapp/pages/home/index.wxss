.home {
  background-color: #f4f4f4;
}
.home .custom_head {
  width: 750rpx;
  background: #fe5457;
  color: #fff;
  position: fixed;
  top: 0;
  z-index: 999;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.home .flex-row {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.home .head_left {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.home .head_sign,
.home .head_signed {
  margin-right: 30rpx;
  margin-left: 22rpx;
  -webkit-flex-shrink: 0;
      -ms-flex-negative: 0;
          flex-shrink: 0;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.home .head_sign image,
.home .head_signed image {
  width: 60rpx;
  height: 60rpx;
}
.home .head_signed {
  color: #ffffff;
}
.home .head_nav {
  position: absolute;
  width: 100%;
  text-align: center;
}
.home .select-city {
  color: #ffffff;
  font-size: 32rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  padding-left: 16rpx;
}
.home .select-city .roll-down {
  width: 26rpx;
  height: 38rpx;
  -webkit-transform: rotate(-90deg);
      -ms-transform: rotate(-90deg);
          transform: rotate(-90deg);
  margin-left: 16rpx;
}
.home .page-bg {
  background: #fe5457;
  width: 100%;
  height: 150rpx;
  border-radius: 0 0 16rpx 16rpx;
}
.home .page-section {
  margin: -130rpx 16rpx 16rpx 16rpx;
  border-radius: 20rpx;
}
.home .swiper-item,
.home .swiper-item-img {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.home .news {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  padding: 0 16rpx 16rpx 16rpx;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.home .news .news-title {
  background: url(data:image/png;base64,) no-repeat 0 center;
  background-size: 36rpx;
  padding-left: 46rpx;
  font-size: 30rpx;
  font-size: #333333;
}
.home .news .news-line {
  margin: 0 16rpx;
  color: #bbbbbb;
}
.home .news .news-txt {
  color: #808080;
}
.home .news .news-btn {
  margin-left: 16rpx;
  color: #f2270c;
  border: 1rpx solid #f2270c;
  border-radius: 8rpx;
  padding: 4rpx 6rpx;
  font-size: 26rpx;
}
.home .page-nav {
  background: #ffffff;
  border-radius: 12rpx;
  margin: 0 16rpx 16rpx 16rpx;
  padding: 16rpx;
}
.home .page-nav-top {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
}
.home .page-nav-item {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.home .page-nav-img {
  width: 100rpx;
  height: 100rpx;
}
.home .page-nav-text {
  color: #333333;
  font-size: 28rpx;
  margin-top: 6rpx;
}
