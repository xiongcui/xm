.home {
  background-color: #f6f6f6;
}
.home .custom_head {
  width: 750rpx;
  background: #fe5457;
  color: #fff;
  position: fixed;
  top: 0;
  z-index: 999;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.home .flex-row {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.home .head_left {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.home .head_sign,
.home .head_signed {
  margin-right: 30rpx;
  margin-left: 22rpx;
  -webkit-flex-shrink: 0;
      -ms-flex-negative: 0;
          flex-shrink: 0;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.home .head_sign image,
.home .head_signed image {
  width: 60rpx;
  height: 60rpx;
}
.home .head_signed {
  color: #ffffff;
}
.home .head_nav {
  position: absolute;
  width: 100%;
  text-align: center;
}
.home .select-city {
  color: #ffffff;
  font-size: 32rpx;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  padding-left: 16rpx;
}
.home .select-city .roll-down {
  width: 26rpx;
  height: 38rpx;
  -webkit-transform: rotate(-90deg);
      -ms-transform: rotate(-90deg);
          transform: rotate(-90deg);
  margin-left: 16rpx;
}
.home .page-bg {
  background: #fe5457;
  width: 100%;
  height: 150rpx;
  border-radius: 0 0 16rpx 16rpx;
}
.home .page-section {
  margin: -130rpx 16rpx 16rpx 16rpx;
  border-radius: 20rpx;
}
.home .swiper-item,
.home .swiper-item-img {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
