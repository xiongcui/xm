  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  border-top: 1rpx solid #f5f5f5;
  padding: 30rpx 0;
}
.select_block .clear {
  display: inline-block;
  width: 200rpx;
  height: 64rpx;
  text-align: center;
  background-color: #f1f1f1;
  border: 1rpx solid #f1f1f1;
  color: #333;
  border-radius: 8rpx;
  font-size: 26rpx;
  line-height: 64rpx;
  margin-right: 16rpx;
}
.select_block .confirm {
  display: inline-block;
  width: 200rpx;
  height: 64rpx;
  text-align: center;
  background-color: #ff5355;
  border: 1rpx solid #ff5355;
  color: #fff;
  border-radius: 8rpx;
  font-size: 26rpx;
  line-height: 64rpx;
}
