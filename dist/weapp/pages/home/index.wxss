  display: flex;
}

.sizer_block {
  height: 70rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.gradient {
  background-image: -webkit-gradient(linear, left top, right top, from(hsla(0deg, 0%, 100%, 0)), to(#fff));
  background-image: -webkit-linear-gradient(left, hsla(0deg, 0%, 100%, 0), #fff);
  background-image: -o-linear-gradient(left, hsla(0deg, 0%, 100%, 0), #fff);
  background-image: linear-gradient(90deg, hsla(0deg, 0%, 100%, 0), #fff);
  height: 50rpx;
  left: -30rpx;
  position: absolute;
  top: 19rpx;
  width: 30rpx;
  z-index: 91;
}

.gradient2 {
  background-image: -webkit-gradient(linear, left top, right top, from(hsla(0deg, 0%, 100%, 0)), to(#f7f8fa));
  background-image: -webkit-linear-gradient(left, hsla(0deg, 0%, 100%, 0), #f7f8fa);
  background-image: -o-linear-gradient(left, hsla(0deg, 0%, 100%, 0), #f7f8fa);
  background-image: linear-gradient(90deg, hsla(0deg, 0%, 100%, 0), #f7f8fa);
}

