.custom_head {
  width: 750rpx;
  background: #fe5457;
  color: #fff;
  position: fixed;
  top: 0;
  z-index: 999;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.flex-row {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.head_sign,
.head_signed {
  margin-right: 30rpx;
  margin-left: 22rpx;
  -webkit-flex-shrink: 0;
      -ms-flex-negative: 0;
          flex-shrink: 0;
  background: #ffffff;
  border-radius: 30rpx;
  color: #fe5457;
  line-height: 100%;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  font-size: 22rpx;
  padding: 0 16rpx;
}

.head_sign image,
.head_signed image {
  width: 22rpx;
  height: 22rpx;
  margin-right: 4rpx;
}

.head_signed {
  background: #f8878f;
  color: #ffffff;
}

.head_nav {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.nav_item {
  margin-right: 16rpx;
}

.search {
  background: url(data:image/png;base64,) no-repeat 16rpx center #feadb4;
  background-size: 30rpx;
  border-radius: 30rpx;
  padding-left: 56rpx;
  width: 70%;
}

.search .search-input {
  margin: 0 4rpx;
  height: 100%;
  color: #ffffff;
  font-size: 26rpx;
}

.search .placeholder-style {
  color: #ffffff;
}
.home {
  padding: 30rpx;
}
